{
    "theme_color": "#f8e71c",
    "background_color": "#88d23c",
    "display": "standalone",
    "scope": "/",
    "start_url": "/",
    "name": "Food",
    "short_name": "Food app",
    "description": "best foods in the world",
    "icons": [
      {
        "src": "/icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "/icon-256x256.png",
        "sizes": "256x256",
        "type": "image/png"
      },
      {
        "src": "/icon-384x384.png",
        "sizes": "384x384",
        "type": "image/png"
      },
      {
        "src": "/maskable.png",
        "sizes": "500x500",
        "type": "image/png"
      }
    ],
    "prefer_related_applications": true,
    "protocol_handlers": [
      {
        "protocol": "web+https",
        "name": "My App",
        "url": "/open?url={%s}"
      }
    ],
    "display_override": {
      "show": true,
      "controls": {
        "position": "top-right"
      },
      "overlay": {
        "position": "top"
      },
      "title": {
        "color": "#ffffff",
        "background_color": "#000000"
      }
    }
  }